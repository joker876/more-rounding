{
  "name": "more-rounding",
  "version": "4.2.0",
  "description": "A library that offers more number rounding options for JavaScript and TypeScript",
  "main": "dist/index.min.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/*"
  ],
  "scripts": {
    "test": "tsc && npx jasmine",
    "start": "ts-node-dev --respawn src/index.ts",
    "build": "rmdir /s /Q dist & tsc && terser dist/index.js --output dist/index.min.js --config_file terserconfig.json --comments /^\\*\\n/ && cd dist && del /f index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/joker876/more-rounding.git"
  },
  "keywords": [
    "round",
    "rounding",
    "Math",
    "math",
    "math.round"
  ],
  "devDependencies": {
    "@types/jasmine": "^4.0.3",
    "jasmine": "^4.1.0",
    "jasmine-ts-console-reporter": "^3.1.1",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.6.3"
  },
  "author": "joker876",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/joker876/more-rounding/issues"
  },
  "homepage": "https://github.com/joker876/more-rounding#readme"
}
